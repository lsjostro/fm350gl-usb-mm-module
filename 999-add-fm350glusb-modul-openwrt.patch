+static void
+get_ip_config_context_free(GetIPConfig3GPPContext *ctx)
+{
+  g_object_unref(ctx->primary);
+  g_object_unref(ctx->modem);
+  g_object_unref(ctx->ipv4_config);
+  g_object_unref(ctx->ipv6_config);
